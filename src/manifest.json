{
  "manifest_version": 3,
  "name": "Bookmarks",
  "version": "1.0",
  "description": "Bookmark the current page with tags and store it in Supabase.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "img/books.png"
  }
}